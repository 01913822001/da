function assert_equal(expected, got)
  if expected == got
    printl("passed: ", expected)
  else
    printl("Assertion failed\nExpected: ", expected, "\nGot: ", got)
  end
end

assert_equal( 1    , 1    )
assert_equal( 3    , 1+2  )
assert_equal( "a"  , "a"  )
assert_equal( true , 1==1 )
